{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "remoteCache": {
    "timeout": 300,
    "uploadTimeout": 300
  },
  "tasks": {
    "build": {
      "dependsOn": ["^build", "codegen"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "generated/**", "dist/**"]
    },
    "bundle": {
      "dependsOn": ["^bundle"],
      "outputs": ["generated/**", "dist/**"]
    },
    "codegen": {
      "dependsOn": ["^codegen"],
      "outputs": ["generated/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck", "codegen"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["codegen"]
    },
    "format:check": {}
  }
}
